<GlossaryTerm id="CDR0000654999">
  <TermName>reduction</TermName>
  <TermPronunciation>(ree-DUK-shun)</TermPronunciation>
  <TermDefinition>
    <DefinitionText>A chemical
          reaction that takes place when a substance comes into
          contact with hydrogen or another reducing
          substance.</DefinitionText>
    <Dictionary>Cancer.gov</Dictionary>
    <Audience>Patient</Audience>
  </TermDefinition>
  <MediaLink ref="CDR0000726272" type="audio/mpeg" alt="Pronunciation of dictionary term &quot;reduction&quot;" language="en" id="_3"/>
  <MediaLink ref="CDR0000726273" type="audio/mpeg" alt="Pronunciation of dictionary term &quot;reducción&quot;" language="es" id="_4"/>
  <SpanishTermName>reducción</SpanishTermName>
  <SpanishTermDefinition>
    <DefinitionText>Reacción química que se produce
cuando una sustancia se pone en contacto con el hidrógeno u
otra sustancia reductora.</DefinitionText>
    <Dictionary>Cancer.gov</Dictionary>
    <Audience>Patient</Audience>
  </SpanishTermDefinition>
  <DateFirstPublished>2009-11-17</DateFirstPublished>
</GlossaryTerm>
